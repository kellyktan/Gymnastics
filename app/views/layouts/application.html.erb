<!DOCTYPE html>
<html>
<head>
  <title>Gymnastics</title>
 
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
   <link rel="stylesheet" type="text/css" ref="../bootstrap.css">
  <script src="../bootstrap.js"></script>
</head>
<body>

  <nav class="navbar navbar-default " role="navigation">
  <!-- Brand and toggle get grouped for better mobile display -->
  <!--<div class="navbar-header">
    <a class="navbar-brand" href="#">Gymnastics</a>
  </div>-->

    <ul class="nav navbar-nav">
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Meets<b class="caret"></b></a>
        <ul class="dropdown-menu">
          <li><%= link_to "new meet", new_meet_path %></li>
          <li><%= link_to "meets", meets_path %></li>
        </ul>
      </li>
    </ul>

    <ul class="nav navbar-nav">
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Gymnasts<b class="caret"></b></a>
        <ul class="dropdown-menu">
          <li><%= link_to "new gymnast", new_gymnast_path %></li>
          <li><%= link_to "gymnasts", gymnasts_path %></li>
        </ul>
      </li>
    </ul>
    
    <ul class="nav navbar-nav">
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Scores<b class="caret"></b></a>
        <ul class="dropdown-menu">
          <li><%= link_to "new score", new_score_path %></li>
          <li><%= link_to "scores", scores_path %></li>
        </ul>
      </li>
    </ul>
    
    <ul class="nav navbar-nav">
    	<% if host_signed_in? %>
    	<li>
    		<%=link_to('Logout',destroy_host_session_path, :method => :delete)%>
    	</li>
    	<% else %>
    	<li>
    		<%=link_to('Login',new_host_session_path)%>
    	</li>
    	<% end %>
    </ul>
</nav>

<!--<p class="notice"><%= notice%></p>
<p class="alert"><%= alert%></p>-->

<%= yield %>

<script>
  $('.dropdown-toggle').dropdown();
</script>
</body>
</html>