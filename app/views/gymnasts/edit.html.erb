<%= form_for(@gymnast) do |f| %>
	<div class="field">
		<p>Name</p>
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<p>College</p>
		<%= f.text_field :college %>
	</div>
	<div class="field">
		<p>Class</p>
		<%= f.number_field :classz, :min => 1995, :max => 2030 %>
	</div>
	<div class="field">
		<p>Meet</p>
		<%= f.collection_select :meet_id, Meet.all, :id, :name %>
	</div>
	
	<div class="actions">
		<%= f.submit %>
	</div>
<% end %>

